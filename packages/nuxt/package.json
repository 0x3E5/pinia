{
  "name": "@pinia/nuxt",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./dist/*": "./dist/*"
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "version": "0.1.9",
  "description": "Nuxt Module for pinia",
  "keywords": [
    "pinia",
    "nuxt",
    "vue",
    "vuex",
    "store"
  ],
  "homepage": "https://github.com/vuejs/pinia/tree/v2/packages/nuxt#readme",
  "bugs": {
    "url": "https://github.com/vuejs/pinia/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/pinia.git"
  },
  "funding": "https://github.com/sponsors/posva",
  "license": "MIT",
  "author": {
    "name": "Eduardo San Martin Morote",
    "email": "posva13@gmail.com"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . -l @pinia/nuxt -r 1",
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "3.0.0-rc.4",
    "vue-demi": "*"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/types": "^2.15.8",
    "jiti": "^1.14.0",
    "nuxt": "^3.0.0-rc.4",
    "pinia": "^2.0.0"
  },
  "peerDependencies": {
    "pinia": ">=2.0.14"
  },
  "publishConfig": {
    "access": "public"
  }
}
